generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Appointment {
  id          String   @id @default(uuid())
  client_name String
  client_phone String
  date        String
  time        String
  service     String
  createdAt   DateTime @default(now())
}
